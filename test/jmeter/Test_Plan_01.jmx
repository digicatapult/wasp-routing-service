          <stringProp name="script">import org.apache.kafka.clients.producer.KafkaProducer
import org.apache.kafka.clients.producer.ProducerRecord
import net.minidev.json.JSONObject
import net.minidev.json.JSONArray
import java.nio.file.Files
import java.nio.file.Paths

def props = new Properties()
props.put(&quot;bootstrap.servers&quot;, &quot;localhost:9092&quot;)
props.put(&quot;acks&quot;, &quot;0&quot;)
props.put(&quot;retries&quot;, 0)
props.put(&quot;batch.size&quot;, 16384)
props.put(&quot;linger.ms&quot;, 1)
props.put(&quot;buffer.memory&quot;, 33554432)
props.put(&quot;key.serializer&quot;, &quot;org.apache.kafka.common.serialization.StringSerializer&quot;)
props.put(&quot;value.serializer&quot;, &quot;org.apache.kafka.common.serialization.StringSerializer&quot;)

def producer = new KafkaProducer(props)

def bytes = new String(Files.readAllBytes(Paths.get(&quot;./test/data/900kb.txt&quot;)))

def messageKey = &quot;321&quot;

def jsonObject = new JSONObject()
jsonObject.put(&quot;video&quot;, bytes.toString())

def jsonMessage = new JSONObject()
jsonMessage.put(&quot;key&quot;, messageKey)
jsonMessage.put(&quot;value&quot;, jsonObject.toJSONString())

def jsonArray = new JSONArray()
jsonArray.add(jsonMessage)

producer.send(new ProducerRecord(&quot;raw-payloads&quot;, messageKey, jsonArray.toJSONString()))

producer.close()
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
